<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewRes.SharedStrings.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("head", required: false)

    <meta name="description" content="The description of my page" />
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink(ViewRes.SharedStrings.Title, "Index", "Sednices", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink(ViewRes.SharedStrings.Pocetna, "Index", "Sednices")</li>
                    <li>@Html.ActionLink(ViewRes.SharedStrings.Sednice, "Index", "Sednices")</li>
                    <li>@Html.ActionLink(ViewRes.SharedStrings.Dokumenta, "", "")</li>
                    <li>@Html.ActionLink(ViewRes.SharedStrings.Administracija, "Administracija", "Sednices")</li>
                </ul>
                @Html.Partial("_CultureChooser")
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/helper")
    @RenderSection("scripts", required: false)
</body>
</html>
